{
  "name": "reporting_agent_backend",
  "private": true,
  "version": "1.0.1",
  "description": "Reporting Agent Backend Service",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "docker:build": "docker build -t reporting-agent-backend:$(node -p \"require('./package.json').version\") .",
    "docker:tag": "docker tag reporting-agent-backend:$(node -p \"require('./package.json').version\") reporting-agent-backend:latest",
    "publish_dev": "git checkout dev && git pull && git merge main && git push origin dev && git checkout main",
    "publish_prod": "git checkout prod && git pull && git merge main && git push origin prod && git checkout main"
  },
  "dependencies": {
    "@apollo/server": "5.0.0",
    "@as-integrations/express5": "1.1.2",
    "@aws-sdk/client-s3": "3.927.0",
    "@clickhouse/client": "1.12.1",
    "@langchain/anthropic": "1.0.0",
    "@langchain/aws": "1.0.0",
    "@langchain/classic": "1.0.0",
    "@langchain/core": "1.0.1",
    "@langchain/langgraph": "1.0.1",
    "@langchain/langgraph-checkpoint-postgres": "1.0.0",
    "@langchain/openai": "1.0.0",
    "@nestjs/apollo": "13.2.1",
    "@nestjs/common": "11.0.3",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.3",
    "@nestjs/graphql": "13.2.0",
    "@nestjs/mongoose": "11.0.0",
    "@nestjs/platform-express": "11.0.3",
    "@nestjs/schedule": "5.0.0",
    "axios": "1.7.9",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "graphql": "16.11.0",
    "graphql-subscriptions": "3.0.0",
    "graphql-type-json": "0.3.2",
    "graphql-ws": "6.0.4",
    "ioredis": "5.5.0",
    "langchain": "1.0.1",
    "mongoose": "8.9.5",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "typeorm": "0.3.26",
    "uuid": "8.3.2",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.0",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.3",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.10.8",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.7",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.21.0",
    "@typescript-eslint/parser": "8.21.0",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "rimraf": "6.1.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
